function validationMsg() {
                    if (jQuery("#retilLicenseform").validationEngine('validate') === false) {
                        jQuery(".formErrorContent").css('display', 'none');
                        jQuery(".formErrorArrow").css('display', 'none');
                        alert("Please fill all the mandatory fields");
                    }
                }
            function checkValidations() {
                var inputs = new Array(valid("userName", "userName"), valid("email", "mail"), valid("userId", "userId"), valid("password", "password"), valid("repassword", "repassword"),
                        valid("orgid", "orgid"), valid("accesstype", "accesstype"), valid("officelevel", "officelevel"),
                        valid("mobile", "mobileno"), valid("role", "role"),
                        valid("dob", "dob"), valid("state", "State"), valid("address", "address"));

                var divs = new Array("userNameError", "emailError", "userIdError", "passwordError", "repasswordError", "orgidError", "accesstypeError", "officelevelError", "mobileError", "roleError", "dobError", "stateError", "addressError");

                $("#mobile").change(function () {
                    var mobno = $(this).val();
                    if (parseInt(mobno.length) == 10) {
                        inputs.push(true);
                        divs.push('mobileError');
                    }
                    else {
                        inputs.push(false);
                        divs.push('mobileError');
                    }
                });
               
                if ($('#divexid').is(':visible')) {
                    var divChk = valid("division", "division");
                    inputs.push(divChk);
                    divs.push('divisionError');
                }

                if ($('#distexid').is(':visible')) {
                    var distChk = valid("district", "district");
                    inputs.push(distChk);
                    divs.push('districtError');
                }
                if (checkFields(inputs, divs) == false) {
                    return false;
                }
            }
            function calculateAge() {
                debugger;
                
                var yrDif = document.getElementById("txtCONSULTANT_AGE").value
                if (yrDif < 21) {
                    document.getElementById("txtDATE_OF_BIRTH").value = "";
//                    document.getElementById('dobError').style.display = "block";
                    alert("Age should be greater than 21 years");
                      document.getElementById("BodyContent_txtDATE_OF_BIRTH").focus();
                }
                else {
//                    document.getElementById('dobError').style.display = "none";
                    document.getElementById("age").value = yrDif;
                }
            }
            function browseFile(slno) {
                document.getElementById('field' + slno).click();
            }
            function setAttachName(slno) {
                var filenm = document.getElementById('field' + slno).value;
                var filenmArr = filenm.split(".");
                document.getElementById('attachname' + slno).value = filenmArr[0];
            }
            function browseImage(image) {
                document.getElementById(image).click();
            }
            function imageUpload(image, imageLbl) {
                var imgText = document.getElementById(image).value;
                var filename = imgText.replace(/^.*[\\\/]/, '');
                var imgTextArr = filename.split(".");
                var imgTxtFTb = imgTextArr[0];
                if (parseInt(imgTxtFTb.length) > 45) {
                    imgTxtFTb = imgTxtFTb.substring(0, 45);
                }
                document.getElementById(imageLbl).value = imgTxtFTb;
            }
           function chkPassword() {
                var password = document.getElementById("password").value;
                var repassword = document.getElementById("repassword").value;
                if (password) {
                    var strongRegex = new RegExp("^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-7])(?=.*[!@#\$%\^&\*])(?=.{6,})");
//                    if(password==null && repassword!=null){
//                        alert("Pls Enter password first");
//                          $('#password').val('');
//                          return true;
//                    }
//                    alert(password+"               ---             "+strongRegex.test(password))
                    if (strongRegex.test(password)) {
//                        document.getElementById('passwordError').style.display = "none";
                        if (repassword) {
                            if (password === repassword) {
//                                document.getElementById('repasswordError').style.display = "none";
                                return true;
                            }
                            
                            else {
//                                document.getElementById('repasswordError').style.display = "block";
                                alert("Passwords Don't Match");
                                $('#repassword').val('');
                                return false;
                            }
                        }
                    }
                    else {
                        $('#password').val('');
                        $('#repassword').val('');
                        alert("Password should have minimum 8 characters in length. At least one character in upper case, lower case & Numeric and special characters! ");
//                        document.getElementById('passwordError').style.display = "block";
                        return false;
                    }
                }
                if(repassword){
                    if(password==null || repassword!=null){
                        alert("Please Enter password first");
                          $('#repassword').val('');
                          return false;
                    }
                }
           }
          
   
            </script>
